<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/inicioFragment">

    <fragment
        android:id="@+id/primerFragment"
        android:name="com.example.projecteuf1.PrimerFragment"
        android:label="fragment_primer"
        tools:layout="@layout/fragment_primer" />
    <fragment
        android:id="@+id/segundoFragment"
        android:name="com.example.projecteuf1.SegundoFragment"
        android:label="fragment_segundo"
        tools:layout="@layout/fragment_segundo" >
        <action
            android:id="@+id/action_segundoFragment_to_mostrarEquipoFragment"
            app:destination="@id/mostrarEquipoFragment" />
        <action
            android:id="@+id/action_segundoFragment_to_nuevoEquipoFragment"
            app:destination="@id/nuevoEquipoFragment" />
    </fragment>
    <fragment
        android:id="@+id/inicioFragment"
        android:name="com.example.projecteuf1.InicioFragment"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" />
    <fragment
        android:id="@+id/mostrarEquipoFragment"
        android:name="com.example.projecteuf1.MostrarEquipoFragment"
        android:label="fragment_mostrar_equipo"
        tools:layout="@layout/fragment_mostrar_equipo" />
    <fragment
        android:id="@+id/nuevoEquipoFragment"
        android:name="com.example.projecteuf1.NuevoEquipoFragment"
        android:label="fragment_nuevo_equipo"
        tools:layout="@layout/fragment_nuevo_equipo" />
</navigation>